<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/welcomeFragment">

    <fragment
        android:id="@+id/PublicationsFragment"
        android:name="com.example.multywork.presentation.fragments.PublicationsFragment"
        android:label="@string/text_label_publishes"
        tools:layout="@layout/fragment_publishes" />
    <fragment
        android:id="@+id/AddFragment"
        android:name="com.example.multywork.presentation.fragments.AddFragment"
        android:label="@string/label_add"
        tools:layout="@layout/fragment_add" />
    <fragment
        android:id="@+id/ProfileFragment"
        android:name="com.example.multywork.presentation.fragments.ProfileFragment"
        android:label="@string/text_label_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/RequestFragment"
        android:name="com.example.multywork.presentation.fragments.RequestFragment"
        android:label="@string/label_request"
        tools:layout="@layout/fragment_request" />
    <fragment
        android:id="@+id/InfoFragment"
        android:name="com.example.multywork.presentation.fragments.InfoFragment"
        android:label="@string/text_label_info"
        tools:layout="@layout/fragment_info" >
        <action
            android:id="@+id/action_InfoFragment_to_WriteToUsFragment"
            app:destination="@id/WriteToUsFragment" />
    </fragment>
    <fragment
        android:id="@+id/SignInFragment"
        android:name="com.example.multywork.presentation.fragments.SignInFragment"
        android:label="@string/welcome"
        tools:layout="@layout/fragment_sign_in"/>
    <fragment
        android:id="@+id/RegistrationFragment"
        android:name="com.example.multywork.presentation.fragments.RegistrationFragment"
        android:label="@string/registration_title"
        tools:layout="@layout/fragment_registration"/>
    <fragment
        android:id="@+id/LoginFragment"
        android:name="com.example.multywork.presentation.fragments.LoginFragment"
        android:label="@string/login_title"
        tools:layout="@layout/fragment_login"/>
    <fragment
        android:id="@+id/PasswordResetFragment"
        android:name="com.example.multywork.presentation.fragments.PasswordResetFragment"
        android:label="@string/pass_reset"
        tools:layout="@layout/fragment_password_reset"/>
    <fragment
        android:id="@+id/AddFragment3"
        android:name="com.example.multywork.presentation.fragments.AddFragment3"
        android:label="AddFragment3" />
    <fragment
        android:id="@+id/AddFragment2"
        android:name="com.example.multywork.presentation.fragments.AddFragment2"
        android:label="AddFragment2" />
    <fragment
        android:id="@+id/WriteToUsFragment"
        android:name="com.example.multywork.presentation.fragments.WriteToUsFragment"
        android:label="@string/write_to_us" />
    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.example.multywork.presentation.fragments.WelcomeFragment"
        android:label="WelcomeFragment" >
        <action
            android:id="@+id/action_welcomeFragment_to_settingsSkills"
            app:destination="@id/settingsSkills" />
    </fragment>
    <fragment
        android:id="@+id/settingsSkills"
        android:name="com.example.multywork.presentation.fragments.SettingsSkills"
        android:label="SettingsSkills" >
        <action
            android:id="@+id/action_settingsSkills_to_settingsProjectFragment"
            app:destination="@id/settingsProjectFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsProjectFragment"
        android:name="com.example.multywork.presentation.fragments.SettingsProjectFragment"
        android:label="SettingsProjectFragment" >
        <action
            android:id="@+id/action_settingsProjectFragment_to_settingsCoverFragment"
            app:destination="@id/settingsCoverFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsCoverFragment"
        android:name="com.example.multywork.presentation.fragments.SettingsCoverFragment"
        android:label="SettingsCoverFragment" >
        <action
            android:id="@+id/action_settingsCoverFragment_to_PublicationsFragment"
            app:destination="@id/PublicationsFragment" />
    </fragment>
</navigation>